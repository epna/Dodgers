<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.sgdm.dodgers.ui.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.sgdm.dodgers.ui.AutonomieFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_autonomie" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.sgdm.dodgers.ui.MoteurFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_moteur" />

    <fragment
        android:id="@+id/navigation_gauge"
        android:name="com.sgdm.dodgers.ui.DashboardDisplay"
        android:label="@string/synthese"
        tools:layout="@layout/fragment_tdb" />
</navigation>